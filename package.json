{
  "name": "eygle-mediastore-front",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cp config.local.json config.json && vite",
    "build": "cp config.prod.json config.json && vite build && cp config.local.json config.json",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "deploy": "npm run build && scp -r dist/* eygle@192.168.0.21:/var/www/eygle-mediastore-front"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.5",
    "@mdi/font": "^7.2.96",
    "@mdi/svg": "^7.2.96",
    "@vueuse/core": "^10.3.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "copy-to-clipboard": "^3.3.3",
    "reflect-metadata": "^0.1.13",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vuetify": "^3.3.9"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.64.1",
    "typescript": "~5.0.4",
    "unplugin-vue-macros": "^2.4.0",
    "vite": "^4.3.9",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.6.5"
  }
}
